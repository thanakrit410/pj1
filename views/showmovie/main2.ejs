<%- include("../in/header.ejs") %>
<link rel="stylesheet" href="/st/index.css">
<body>
    <div class="container">
      <div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
        <% showmovie.forEach(function(showmovie){ %>
        <div class="col">
          <div class="box">
            <img src="<%= showmovie.image %>" height="200px" width="200px">
            <div class="card-body">
              <p class="card-text">name:<%= showmovie.name %></p>
              <p class="card-text">date:<%= showmovie.date %></p>
              <div class="d-flex justify-content-between align-items-center">
                <div class="btn-group">
                  <a href="/Pagemovie/<%= showmovie._id %>" class="btn btn-sm btn-outline-secondary">View</a>

                  <!--<button type="button" class="btn btn-sm btn-outline-secondary">Edit</button> -->
                </div>
                <!--<small class="text-muted">9 mins</small> -->
              </div>
            </div>
          </div>
        </div>
        <% }); %> 
       </div>
    </div> 
</body>
<%- include("../in/footer.ejs") %>